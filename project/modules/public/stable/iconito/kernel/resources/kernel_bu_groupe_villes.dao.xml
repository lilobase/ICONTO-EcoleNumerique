<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE daodefinition SYSTEM "dao.dtd">
<daodefinition version="1">
	<datasource>
		<tables>
			<table name="kernel_bu_groupe_villes" tablename="kernel_bu_groupe_villes" primary="yes"/>
		</tables>
	</datasource>
	<properties>
		<property name="id_grv"  fieldname="id_grv"  type="autoincrement" table="kernel_bu_groupe_villes" pk="yes"/>
		<property name="nom_groupe"   fieldname="nom_groupe"   type="string" table="kernel_bu_groupe_villes" pk="no"/>
		<property name="date_creation" fieldname="date_creation" type="date"    table="kernel_bu_groupe_villes" pk="no"/>
 	</properties>
 	<methods>
		<method name="findAllOrderByName" type="select">
			<order>
				<orderitem property="nom_groupe" way="asc" />
			</order>
		</method>
	</methods>
</daodefinition>
